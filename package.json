{
  "name": "lyi-dev",
  "version": "1.1.0",
  "description": "",
  "main": "src/server/index.js",
  "scripts": {
    "clean": "rm -rf public",
    "server": "node src/server/index.js",
    "server:dev": "nodemon --watch src/server --exec npm run server",
    "test": "jest",
    "build": "npm run clean && webpack --mode production",
    "build:webpack": "npm run clean && webpack --mode development",
    "lint": "eslint src/**/**/*.js src/**/*.js",
    "fmt:js": "prettier --config js.prettierrc --write src/**/*.{js,jsx} src/**/**/*.{js,jsx}",
    "fmt:style": "prettier --config style.prettierrc --write src/**/*.scss src/**/**/*.scss",
    "fmt": "npm run fmt:js && npm run fmt:style",
    "start": "npm run build:webpack && npm run server",
    "release": "npm version",
    "precommit": "lint-staged",
    "prepush": "npm run fmt && npm test",
    "postrelease": "npm publish && git push origin HEAD --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luyi985/lyi-dev.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luyi985/lyi-dev/issues"
  },
  "homepage": "https://github.com/luyi985/lyi-dev#readme",
  "dependencies": {
    "apollo-server-express": "^2.0.0",
    "express": "^4.16.3",
    "foundation-sites": "^6.5.0-rc.2",
    "graphql": "^0.13.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "superagent": "^3.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cors": "^2.8.4",
    "css-loader": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "lint-staged": "^7.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "nodemon": "^1.18.3",
    "prettier": "^1.13.7",
    "regenerator-runtime": "^0.12.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8"
  },
  "bin": {
    "lyi-dev": "./cli.js"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileTransformer.js"
    }
  },
  "lint-staged": {
    "src/**/**/*.{js,jsx}": "prettier --config js.prettierrc --write",
    "src/**/**/*.{scss,css}": "prettier --config style.prettierrc --write",
    "src/**/*.{js,jsx}": "prettier --config js.prettierrc --write",
    "src/**/*.{scss,css}": "prettier --config style.prettierrc --write"
  }
}
